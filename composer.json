{
    "name": "stechstudio/beankeep",
    "description": "PHP Library to facilitate double-entry bookkeeping",
    "type": "library",
    "version": "0.1.0-dev",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "psy/psysh": "^0.11",
        "phpstan/phpstan": "^1.9"
    },
    "license": "TBD",
    "autoload": {
        "psr-4": {
            "STS\\Beankeep\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\Beankeep\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Zeb DeOs",
            "email": "zebulin@stechstudio.com"
        }
    ],
    "scripts": {
        "psysh": [
            "Composer\\Config::disableProcessTimeout",
            "psysh"
        ],
        "repl": [
          "@psysh"
        ],
        "stan": [
          "phpstan analyse --level=9 src tests"
        ],
        "test": [
          "phpunit"
        ]
    }
}
